<?xml version="1.0" ?>
<robot name="bookcase"  xmlns:xacro="http://www.ros.org/wiki/xacro">
    <link name="bottom_shelf">
      <inertial>
        <origin
            xyz="0 0 0"
            rpy="0 0 0" />
        <mass
            value="0.1" />
        <inertia
            ixx="1"
            ixy="0"
            ixz="0"
            iyy="1"
            iyz="0"
            izz="1" />            
      </inertial>
    <visual>
      <geometry>
        <mesh filename="lab_small.stl"/>
      </geometry>
      <material name="panda_white"/>
    </visual>
    </link>
    <link name="top_shelf">
      <inertial>
        <origin
            xyz="0 0 0"
            rpy="0 0 0" />
        <mass
            value="0.1" />
        <inertia
            ixx="1"
            ixy="0"
            ixz="0"
            iyy="1"
            iyz="0"
            izz="1" />            
      </inertial>
    <visual>
      <geometry>
        <mesh filename="up_shelf.stl"/>
      </geometry>
      <material name="panda_white"/>
    </visual>
    </link>
  <joint name="up_joint" type="fixed">
    <parent link="bottom_shelf"/>
    <child link="top_shelf"/>
    <origin rpy="0 0 0" xyz="0 0 0.5"/>
    <axis xyz="0 0 0"/>
  </joint>

</robot>

